{
  "name": "musicplayerwebpkg",
  "type": "wasm-webpkg",
  "output": "MusicPlayer.webpkg",
  "sources": [
    "Main.cpp",
    "MusicPlayer.cpp",
    "MusicPlayer.h",
    "NavPanel.cpp",
    "NavPanel.h",
    "PlayerPanel.cpp",
    "PlayerPanel.h",
    "PlayerButton.cpp",
    "PlayerButton.h",
    "PlayerProgress.cpp",
    "PlayerProgress.h",
    "PlayerVolume.cpp",
    "PlayerVolume.h",
    "Playlist.cpp",
    "Playlist.h",
    "PodcastEpisode.cpp",
    "PodcastEpisode.h"
  ],
  "wwwroot": "wwwroot",
  "cssfile": "Main.css",
  "shellfile": "Main.html",
  "configurations": [
    {
      "name": "debug",
      "cppflags": "-g -O0 -DDEBUG -D_DEBUG -I $(SolutionDir)",
      "linkflags": "-g -O0"
    },
    {
      "name": "release",
      "cppflags": "-O1 -DRELEASE -D_RELEASE -I $(SolutionDir)",
      "linkflags": "-O1"
    }
  ]
}