{
  "name": "websitewebpkg",
  "type": "wasm-webpkg",
  "output": "Website.webpkg",
  "sources": [
    "ApplicationView.cpp",
    "ApplicationView.css",
    "ApplicationView.h",
    "BigTeaser.cpp",
    "BigTeaser.css",
    "BigTeaser.h",
    "Main.cpp",
    "Main.css",
    "ThreeChoices.cpp",
    "ThreeChoices.css",
    "ThreeChoices.h",
    "TwoChoices.cpp",
    "TwoChoices.css",
    "TwoChoices.h",
    "WebPageBlock.cpp",
    "WebPageBlock.h",
    "WebPageBody.cpp",
    "WebPageBody.css",
    "WebPageBody.h",
    "WebPageFooter.cpp",
    "WebPageFooter.css",
    "WebPageFooter.h",
    "WebPageHeader.cpp",
    "WebPageHeader.css",
    "WebPageHeader.h"
  ],
  "wwwroot": "wwwroot",
  "cssfile": "Main.css",
  "shellfile": "Main.html",
  "configurations": [
    {
      "name": "debug",
      "cppflags": "-g -O0 -DDEBUG -D_DEBUG -I $(SolutionDir)",
      "linkflags": "-g -O0"
    },
    {
      "name": "release",
      "cppflags": "-O1 -DRELEASE -D_RELEASE -I $(SolutionDir)",
      "linkflags": "-O1"
    }
  ]
}